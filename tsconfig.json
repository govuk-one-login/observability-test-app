{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "baseUrl": ".",
    "paths": {
      "@/*": ["*"]
    },
    "moduleResolution": "Node",
    "importsNotUsedAsValues": "remove",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noPropertyAccessFromIndexSignature": false,
    "skipLibCheck": true,
    "ignoreDeprecations": "5.0",
  },
  "extends": [],
  "exclude": ["node_modules"],
  "include": ["**/*.ts"]
}